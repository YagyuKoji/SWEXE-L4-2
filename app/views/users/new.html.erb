<h1>登録画面</h1>

<%= render "form" %>

<%= form_tag action: :create do %>
<p><%= label_tag :uid, 'ユーザーID : ' %>
<%= text_field_tag :uid %></p>
<p><%= label_tag :pass, 'パスワード : ' %>
<%= password_field_tag :pass %></p>
<%= submit_tag '登録' %>
<% end %>

<p><%= link_to 'ログイン画面へ', top_main_path %></p>
<p><%= link_to 'ユーザー一覧', users_path %></p>

